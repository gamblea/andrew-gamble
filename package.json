{
  "name": "andrew-gamble",
  "version": "1.0.0",
  "description": "Andrew Gamble Personal Website",
  "main": "src/server/index.js",
  "scripts": {
    "build-posts": "python3 src/client/build-posts.py",
    "update-posts": "export AWS_PROFILE=gamblea && aws s3 sync src/client/posts s3://www.andrewgamble.ca/posts",
    "update-dist": "export AWS_PROFILE=gamblea && aws s3 sync dist  s3://www.andrewgamble.ca/",
    "build": "webpack --mode production",
    "deploy": "npm-run-all --parallel build build-posts --parallel update-posts update-dist",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Andrew Gamble",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-grid-gallery": "^0.5.5",
    "react-images": "^1.1.0-beta.3",
    "react-photo-gallery": "^8.0.0",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "bootstrap": "^4.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
