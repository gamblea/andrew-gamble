{
  "name": "andrew-gamble",
  "version": "1.0.0",
  "description": "Andrew Gamble Personal Website",
  "main": "src/server/index.js",
  "scripts": {
    "build-posts": "python3 src/client/build-posts.py",
    "update-posts": "aws s3 sync src/client/posts s3://www.andrewgamble.com/posts",
    "update-dist": "aws s3 sync dist  s3://www.andrewgamble.com/",
    "refresh_cdn": "aws cloudfront create-invalidation --distribution-id E30N02I5OU9SM1 --paths '/*'",
    "build": "webpack --mode production",
    "deploy": "npm-run-all --sequential build-posts build --parallel update-posts update-dist",
    "start": "node src/server/index.js",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "author": "Andrew Gamble",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-gallery": "^0.5.5",
    "react-images": "^1.1.3",
    "react-photo-gallery": "^8.0.0",
    "react-responsive": "^8.0.3",
    "react-router-dom": "^5.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.3.0",
    "bootstrap": "^4.4.1",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.2",
    "css-loader": "^6.7.3",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.3",
    "npm-run-all": "^4.1.5",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "url-loader": "^4.1.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  }
}
